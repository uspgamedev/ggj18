script/source = "extends Camera2D

var z = 1

func _ready():
	set_process_unhandled_input(true)

func _unhandled_input(event):
	if event.is_action_pressed(\"zoom_in\"):
		z -= 0.1
		z = max(0.1, z)
	elif event.is_action_pressed(\"zoom_out\"):
		z += 0.1
		z = min(2, z)

func _process(delta):
	self.zoom = Vector2(z,z)
"

