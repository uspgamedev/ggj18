script/source = "extends Node2D

func _ready():
	set_process_unhandled_input(true)
	set_process(false)

func _unhandled_input(event):
	if event.is_action_pressed(\"action_play\"):
		$LeaveTween.interpolate_method(self, \"_set_scroll\", 0, 800, 1, Tween.TRANS_LINEAR,
									   Tween.EASE_IN_OUT)
		$LeaveTween.start()
		yield($LeaveTween, \"tween_completed\")
		get_node(\"/root/Progress\").go_to_stage_selector()
		queue_free()

func _set_scroll(y):
	$ParallaxBackground.scroll_offset.y = y
	$GUI.position.y = y
"

